            <div id="list_container1" hidden>
                <title>Marché dans le 1er arrondissement de Paris</title>
                {% for block in section.blocks %}
                {%if block.settings.code == "1" %}
                <div class="row_container">
                    <div class="row_content" onclick="showBox({% increment my_counter %})">
                        <div class="content flex_row_container" style="background-color:{{block.settings.color_bg}}">

                            <div class="image_content">
                                <img src="{{ block.settings.logo | img_url:'master'}}" alt="Image du marché">
                            </div>

                            <div class="body_container">

                                <div class="flex_row_container large_auto_box"
                                    style="background-color:{{block.settings.color_bg}}">

                                    <div class="flex_col_container flex_1_box">
                                        <div class="location_content">
                                            <p style="color:{{block.settings.color_text}}">{{ block.settings.location }}
                                            </p>
                                        </div>

                                        <div class="description_content flex_1_box">
                                            <p style="color:{{block.settings.color_text}}">
                                                {{ block.settings.description }}</p>
                                        </div>
                                    </div>

                                    <div class="btn_container">
                                        <div class="btn_box">
                                            <a href="{{ block.settings.link }}"><button class="btn_choose"
                                                    style="background-color:{{block.settings.color_btton}};color:{{block.settings.color_text_btton}}">{{ block.settings.linktext }}</button></a>

                                        </div>
                                    </div>

                                </div>

                            </div>

                        </div>

                        {% if block.settings.market_product == true %}
                        <div class="hover_container" style="background-color:{{block.settings.col_bg_market}}">
                            <div class="content hover_content">

                                <div class="product_container flex_1_box">
                                    <div class="hover_body">
                                        <img src="{{ block.settings.img_collec1 | img_url:'master'}}"
                                            alt="Product image 1">
                                        <p class="img_title" style="color:{{block.settings.col_text_market1}}">
                                            {{ block.settings.collection1 }}</p>
                                    </div>
                                </div>

                                <div class="product_container flex_1_box">
                                    <div class="hover_body">
                                        <img src="{{ block.settings.img_collec2 | img_url:'master'}}"
                                            alt="Product image 2">
